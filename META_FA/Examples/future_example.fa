Declare Dfa
    States = (s1, s2, s3)
    Initial = s1
    Finals = (s3)
    Table = (
        <s1 a s3>
        <s1 b s2>
        <s1 c s1>
        <s2 c s1>
        <s3 b s2>
    )
End Declare

